#include<stdio.h>
int main()
{
    int n,d1,d2,d3,a,arm;
    printf("Enter a number : ");
    scanf("%d",&n);
    d3=n%10;
    a=n/10;
    d2=a%10;
    d1=a/10;
    arm=(d1*d1*d1)+(d2*d2*d2)+(d3*d3*d3);
    if(arm==n)
    {
        printf("The given number is a armstrong number");
    }
    else
    {
        printf("The given number is not a armstrong number");
    }
    return 0;
}
